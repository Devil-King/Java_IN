//Swap two numbers without third variable
import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
		int a,b;
		a=sc.nextInt();
		b=sc.nextInt();
		a=a+b;
		b=a-b;
		a=a-b;
		//a=10
		//b=20
		//a=a+b -- 30
		//b=a-b -- 10
		//a=a-b -- 20
		System.out.println(a+"    "+b);
	}
}
