public class Main
{
	public static void main(String[] args) {
		int num=8091;
		String s= Integer. toString();
		int temp=0;
		int temp1=0;

		int i=0;
		while(num>0){
		   temp1=temp%10;
		   if(temp1==0){
		       temp1 = 1;
		   }
		   if(temp1==1){
		       temp1 = 0;
		   }
		   for(int j=1;j<=s.length();j++){
		   temp1=temp1+temp1*10;    
		   }
		   temp = temp/10;
		    i++;
		}
		System.out.println(temp1);
	}
}
